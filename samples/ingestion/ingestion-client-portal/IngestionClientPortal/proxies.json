{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "app": {
      "disabled": false,
      "matchCondition": {
        "methods": [ "GET", "OPTIONS" ],
        "route": "/{*path}"
      },
      "backendUri": "https://localhost/api/IngestionClientPortal?file={path}",
      "responseOverrides": {
        "response.headers.Cache-Control": "public,max-age=600"
      }
    }
  }
}